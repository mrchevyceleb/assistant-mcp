{
  "name": "@mattjohnston/assistant-mcp-server",
  "version": "1.0.0",
  "description": "Production-ready remote MCP server for Matt's virtual assistant",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "tsx src/index.ts",
    "start:compiled": "npm run build && node dist/index.js",
    "start:quick": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "migrate": "node scripts/migrate.js"
  },
  "keywords": [
    "mcp",
    "assistant",
    "automation"
  ],
  "author": "Matt Johnston",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "@supabase/supabase-js": "^2.39.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "google-auth-library": "^10.5.0",
    "googleapis": "^170.1.0",
    "node-cron": "^3.0.3",
    "winston": "^3.11.0",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.25.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.5",
    "@types/node-cron": "^3.0.11",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
